*color1: #b32e52

! bright_red
*color9: #e28fa5

! green
*color2: #52b32e

! bright_green
*color10: #a5e28f

! yellow
*color3: #b3902e

! bright_yellow
*color11: #e2cc8f

! blue
*color4: #2e52b3

! bright_blue
*color12: #8fa5e2

! magenta
*color5: #902eb3

! bright_magenta
*color13: #cc8fe2

! cyan
*color6: #2eb390

! bright_cyan
*color14: #8fe2cc

! white
*color7: #bfbfbf

