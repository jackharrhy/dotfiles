#!/bin/bash
echo 'SSH-ing to root@104.131.251.106 via port 443...'
printf '\e[38;6;160m
       _ __
    __( =  =- _
   (-       -  )__- -_
  (  -=  - )   -     _)
 (_-= _(    =-    _=-
  -(     -    -  _)
   -=__(__  _-)-
         -=-\033[0m
\e[38;5;160m^^^^^^^^^^^^^^^^^^^^^^^^\e[38;5;255m
        ___I_
       /\-_--\
      /  \_-__\
      |[]| [] |\033[0m
'
ssh -p 443 root@104.131.251.106
printf '
       _ __
    __( =  =- _
   (-       -  )__- -_
  (  -=  - )   -     _)
 (_-= _(    =-    _=-
  -(     -    -  _)
   -=__(__  _-)-
         -=-
\e[38;5;160mvvvvvvvvvvvvvvvvvvvvvvv\e[39;6;11m
        ___I_
       /\-_--\
      /  \_-__\
      |[]| [] |\033[0m
'
echo 'Session finished!'
